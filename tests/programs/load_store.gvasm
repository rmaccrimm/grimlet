.begin main
    .arm
// init stack
    mov sp, #0x4000

// first test - covers [reg] and [reg, #offset]
    mov r1, #0x330
    ldr r0, [r1]
    ldr r2, =0x56c7c13d // pc-relative load
    cmp r0, r2
    beq s1
    ldr r0, =-1
    push {r0}
    b end
s1:
    mov r3, #1
    push {r3}
    
// reduce by 1 for each new instruction
    .u32fill 192
end: // 0x32c
    nop
